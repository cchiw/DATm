function real pnt(real dist, real p){
    //r^2-y^2=x^2
    real pp = p;
    real rr = dist*dist;
return sqrt(rr-pp*pp);
}

function real get_x(int i){
real pos_x =0;
    if(i==0){ pos_x=0;}
    else if(i<1){ pos_x=0;}//up to 0
